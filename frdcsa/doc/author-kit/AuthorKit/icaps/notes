AUTHORLIST ( <AUTHORLIST> ', ' <AUTHOR> )
AUTHORS :- ( <AUTHOR> | <AUTHOR> ' and ' <AUTHOR> | <AUTHORLIST> ','? ' and ' <AUTHOR> | <AUTHOR> ' et al.' )
INLINE-CITATION :- '(' <AUTHORS> ' ' <YEAR> ')'
ACRONYM-DEFINITION :- <PHRASE> ' (' <ACRONYM> ')'

CITATION :- <ABBREV-AUTHOR> '. ' <YEAR> '.' <TITLE>. <PUBLICATION>

